{
  "name": "ignite-syntax",
  "displayName": "Ignite Syntax Highlighting",
  "description": "Syntax highlighting and icon for .ignite files used in Ignite projects.",
  "version": "0.0.2",
  "publisher": "KingJethro",
  "author": {
    "name": "King Jethro",
    "url": "https://github.com/kingjethro999"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ignite",
        "aliases": [
          "Ignite",
          "ignite"
        ],
        "extensions": [
          ".ignite"
        ],
        "icon": {
          "light": "./icons/icon.svg",
          "dark": "./icons/icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "ignite",
        "scopeName": "source.ignite",
        "path": "./syntaxes/ignite.tmLanguage.json"
      }
    ]
  },
  "icon": "icons/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kingjethro999/ignite"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "vscode": "^1.1.37"
  }
}
